package javaDemo;

import java.util.Arrays;
import java.util.Collections;
import java.util.Scanner;
import java.util.Collections;
public class First {

	public static void main(String[] args) {
	
	String s="vipul khutale";
	String[] s1=s.split(" ");
	String rev="";
	String rev1="";
	String fw="";
	String sw="";
	
	for(int i=s1.length-1; i>=0; i--)
	{
		fw=s1[0];
		sw=s1[1];
	}
	for(int j=fw.length()-1; j<=0;j--)
	{
		rev=rev+fw.charAt(j);
	}
	for(int k=sw.length()-1; k<=0;k--)
	{
		rev1=rev1+fw.charAt(k);
	}
	
	System.out.println(rev+" "+rev1);
	}
}
