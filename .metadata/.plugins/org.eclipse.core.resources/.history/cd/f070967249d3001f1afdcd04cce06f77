package javaDemo;

import java.util.Scanner;

public class First {

	public static void main(String[] args) {
	
	int[] a= {1,3,5,7,9,4,6};
	
	for(int x:a)
	{
		if(x%2==0)
		{
			System.out.println("even no is"+x);
		}
	}
	for(int x:a)
	{
		if(x%2!=0)
		{
			System.out.println("odd no is"+x);
		}
	}
	
	
	}
}
